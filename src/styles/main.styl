html, body
  height: 100%
  width: 100%
  overflow: hidden

.tile
  width: 260px
  position: absolute
  .tile-img
    height: 347px
  .tile-title
    height: 20px
  .tile-desc
    height: 263px
  .tile-img, .tile-title, .tile-desc
    cursor: pointer
    background-color: #E0E0E0

.tile-box
  height: 100%
  width: 100%
  overflow: hidden
